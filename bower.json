{
  "name": "flux-ld",
  "version": "0.0.0",
  "authors": [
    "Tomasz Pluskiewicz <tomasz@t-code.pl>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.1",
    "jsonld": "~0.3.19",
    "reqwest": "~1.1.5",
    "underscore": "~1.7.0",
    "reflux": "~0.2.3",
    "font-awesome": "~4.2.0",
    "requirejs": "~2.1.15",
    "jquery": "~2.1.3",
    "paper-dialog-behavior": "PolymerElements/paper-dialog-behavior#^1.0.0",
    "iron-ajax": "PolymerElements/iron-ajax#~1.0.3",
    "paper-dialog": "PolymerElements/paper-dialog#~1.0.1"
  },
  "overrides": {
    "polymer": {
      "main": [
        "polymer-mini.html",
        "polymer.html",
        "polymer-micro.html"
      ]
    },
    "bootstrap": {
      "main": "dist/**/*.*"
    },
    "webcomponentsjs": {
      "main": "webcomponents*.js"
    },
    "paper-dialog-behavior": {
      "main": [
        "paper-dialog-behavior.html",
        "paper-dialog-common.css"
      ]
    },
    "promise-polyfill":{
      "main": [
        "**/*"
      ]
    },
    "iron-meta":{
      "main": [
        "**/*"
      ]
    },
    "iron-flex-layout":{
      "main": [
        "**/*"
      ]
    },
    "paper-styles":{
      "main": [
        "**/*"
      ]
    },
    "neon-animation":{
      "main": [
        "**/*"
      ]
    },
    "iron-overlay-behavior":{
      "main": [
        "**/*"
      ]
    },
    "web-animations-js":{
      "main": [
        "**/*"
      ]
    }
  },
  "resolutions": {
    "webcomponentsjs": "^0.7.2",
    "polymer": "^1.0.0"
  }
}
