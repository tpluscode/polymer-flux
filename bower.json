{
  "name": "flux-ld",
  "version": "0.0.0",
  "authors": [
    "Tomasz Pluskiewicz <tomasz@t-code.pl>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.1",
    "polymer": "~0.5.2",
    "jsonld": "~0.3.19",
    "reqwest": "~1.1.5",
    "zdk-modal": "~0.3.0",
    "underscore": "~1.7.0",
    "reflux": "~0.2.3",
    "qwest": "~1.5.4",
    "xdomain": "~0.6.17"
  },
  "overrides": {
    "polymer": {
      "main": [
        "polymer.js",
        "polymer.html",
        "layout.html"
      ]
    },
    "bootstrap": {
      "main": "dist/**/*.*"
    },
    "core-icon": {
      "main": "core-icon.*"
    },
    "core-icons": {
      "main": "*.html"
    },
    "core-iconset": {
      "main": [
        "core-iconset.html",
        "*.png"
      ]
    },
    "core-iconset-svg": {
      "main": "core-iconset-svg.html"
    },
    "core-meta": {
      "main": "core-meta.html"
    },
    "core-component-page": {
      "main": "core-component-page.*"
    }
  },
  "devDependencies": {
    "reflux": "~0.2.3"
  }
}
